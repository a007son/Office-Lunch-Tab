  server: {
    proxy: {
      // 本地開發時，將 /.netlify/functions 請求代理到 Netlify Dev Server (預設 8888)
      '/.netlify/functions': {
        target: 'http://localhost:8888',
        changeOrigin: true,
      },
    },
  },
})